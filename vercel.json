{
    "builds": [
      {
        "src": "./src/app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],
    "rewrites": [
      {
        "source": "/views/(.*)",
        "destination": "/src/views/$1"
      }
    ]
  }